@model IEnumerable<Flex_SGM.Models.eoriginator>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutE.cshtml";
}

<h2>Autores</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Supervisor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Areas.Area)
        </th>
        <th>Menú</th>
    </tr>

   @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Supervisor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Areas.Area)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id=item.ID }) |
                @Html.ActionLink("Detalles", "Details", new { id=item.ID }) |
                @Html.ActionLink("Eliminar", "Delete", new { id=item.ID })
            </td>
        </tr>
    }

</table>
